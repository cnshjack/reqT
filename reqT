//load me in the scala REPL with :load reqT
if ($intp.quietRun("""
    org.reqt.Model.interpreter = Some($intp)
  """) == scala.tools.nsc.interpreter.IR.Success) 
println("*** Welcome to reqT ***") else println("Init reqT.Model.interpreter failed.")
{
  $intp.interpret("import scala.language._")
  $intp.interpret("import org.reqt._")
  $intp.interpret("import org.reqt.jacop._")
  $intp.interpret("import org.reqt.csp._")
  $intp.interpret("import org.reqt.util._")
  ()
}
